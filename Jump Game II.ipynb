   "source": [
    "def jump(nums):\n",
    "    res = 0\n",
    "    l = r = 0\n",
    "\n",
    "    while r < len(nums) - 1:\n",
    "        farthest = 0\n",
    "        for i in range(l, r+1):\n",
    "            farthest = max(farthest, i+nums[i])\n",
    "        l = r+1\n",
    "        r = farthest\n",
    "        res += 1\n",
    "    return res"
   ]
  },
