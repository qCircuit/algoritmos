   "source": [
    "def maxProfit(prices):\n",
    "    maxProfit = 0\n",
    "\n",
    "    for i in range(1, len(prices)):\n",
    "        if prices[i] > prices[i-1]:\n",
    "            maxProfit += prices[i] - prices[i-1]\n",
    "\n",
    "    return maxProfit\n",
    "\n",
    "maxProfit(prices)"
   ]
